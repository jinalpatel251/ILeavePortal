<div class="card pmd-card single-col-form" style="margin-top: 150px">
    <form id="leave" method="post" action="" novalidate="novalidate">
        <div class="card-body">
            <div class="row">
                <div class="col-12">
                    <div class="form-group pmd-textfield pmd-textfield-floating-label ">
                        <label for="leave-type">Leave Type</label>
                        <select name="leave-type" id="LeaveType" class="form-control" title="Please select a Leave Type" required="">
                            <option></option>
                            <option>Sick Leave</option>
                            <option>Casual Leave</option>
                            <option>EBL</option>
                            <option>Unpaid Leave</option>
                            <option>Hourly Leave</option>
                        </select><span class="pmd-textfield-focused"></span>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group pmd-textfield pmd-textfield-floating-label">
                        <label class="control-label" for="datepickerstart">Start Date</label>
                        <input type="date" class="form-control" id="Start Date" name="datepickerstart"><span class="pmd-textfield-focused"></span>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group pmd-textfield pmd-textfield-floating-label">
                        <label class="control-label" for="datepickerend">End Date</label>
                        <input type="date" class="form-control" id="EndDate" name="datepickerend"><span class="pmd-textfield-focused"></span>
                    </div>
                </div>
                <div class="col-12">
                    <div class="form-group pmd-textfield pmd-textfield-floating-label">
                        <label>Reason</label>
                        <textarea class="form-control" id="FF" name="reason"></textarea><span class="pmd-textfield-focused"></span>
                    </div>
                </div>
                <div class="col-12">
                    <div class="custom-control custom-checkbox pmd-checkbox">
                        <input class="custom-control-input" type="checkbox" value="" id="IsItaHalfDay" id="inverse_defaultCheck1">
                        <label class="custom-control-label" for="inverse_defaultCheck1">
                            Is It a Half Day?
                        </label>
                    </div>
                </div>
            </div>
        </div>
        <div class="card-footer">
           <button type="submit" class="btn btn-primary pmd-ripple-effect pmd-btn-raised" name="approveleave" onclick="ApproveLeave()" value="Approve">Approve</button>
@*              <button type="submit" class="btn btn-danger pmd-ripple-effect pmd-btn-raised" name="rejectleave" onclick="RejectLeave()" value="Reject">Reject</button> *@          
            <a href="/Admin/Index" class="btn btn-outline-secondary pmd-ripple-effect">Cancel</a>
        </div>
    </form>
</div>



